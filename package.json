		"vite": "^4.5.0",
		"vitest": "^0.34.6",
		"vue": "^3.4.38",
		"vue-tsc": "^2.0.29"
	},
	"dependencies": {
		"lodash.debounce": "^4.0.8",
		"swiper": "^11.1.10",
		"video.js": "^8.17.3",
		"vue3-popper": "^1.5.0"
