		"@vue/test-utils": "^2.4.1",
		"@vue/tsconfig": "^0.4.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.53.0",
		"jsdom": "^22.1.0",
		"lint-staged": "^15.0.2",
		"postcss": "^8.4.41",
