	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./*": "./*"
	},
	"typings": "types/index.d.ts",
	"files": [
		"dist",
		"types",
		"README.md"
	],
	"scripts": {
		"dev-server": "vite",
		"dev-types": "vue-tsc  --noEmit -p tsconfig.app.json --watch",
		"build": "npm run build-lib && npm run build-types",
